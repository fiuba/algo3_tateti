Object subclass: #Celda	instanceVariableNames: 'dir contenido'	classVariableNames: ''	package: 'TTT'!!Celda methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:14'!dir	^dir.! !!Celda methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:08'!dir: aDir 	dir := aDir.! !!Celda methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:32'!contenido: unaFicha 	contenido := unaFicha! !!Celda methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:24'!es: otraDirCelda  	^dir = otraDirCelda.! !!Celda methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:29'!contiene: unaFicha 	^contenido = unaFicha ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Celda class	instanceVariableNames: ''!!Celda class methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:07'!dir: aDir 	|result|	result := self new.		result dir: aDir.		^result.! !Object subclass: #DirCelda	instanceVariableNames: 'fila columna'	classVariableNames: ''	package: 'TTT'!!DirCelda methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:12'!col	^ columna ! !!DirCelda methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:12'!fila	^ fila! !!DirCelda methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:03'!f: unaFila c: unaCol 	fila := unaFila.	columna := unaCol.! !!DirCelda methodsFor: 'comparing' stamp: 'DS 6/24/2021 18:24'!= otraDirCelda 	^(fila = (otraDirCelda fila)) and: [ columna = (otraDirCelda col) ]! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!DirCelda class	instanceVariableNames: ''!!DirCelda class methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:02'!f: unaFila c: unaCol 	|result|	result := self new.		result f: unaFila c: unaCol .		^result.! !Object subclass: #Ficha	instanceVariableNames: 'caracter'	classVariableNames: ''	package: 'TTT'!!Ficha methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:27'!= otraFicha	^caracter = (otraFicha caracter).! !!Ficha methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:31'!caracter	^ caracter! !!Ficha methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:27'!de: aString 	caracter := aString.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Ficha class	instanceVariableNames: ''!!Ficha class methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:26'!de: aString 	|result|	result := self new.		result de: aString.		^result.! !Object subclass: #JugadorAI	instanceVariableNames: 'unJugador'	classVariableNames: ''	package: 'TTT'!!JugadorAI methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:24'!dondeJuego	^unJugador dondeJuego .! !!JugadorAI methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:25'!ficha: unaFicha 	unJugador := JugadorHumano ficha: (Ficha de: unaFicha).	unJugador proximaJugada: (DirCelda f:1 c:4).! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!JugadorAI class	instanceVariableNames: ''!!JugadorAI class methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:19'!ficha: unaFicha	|result|	result := self new.		result ficha: unaFicha.		^result.! !Object subclass: #JugadorHumano	instanceVariableNames: 'ficha miProximaCelda'	classVariableNames: ''	package: 'TTT'!!JugadorHumano methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:14'!initialize 	miProximaCelda := DirCelda f:1 c:1.! !!JugadorHumano methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:20'!proximaJugada: aDirCelda 	miProximaCelda := aDirCelda .! !!JugadorHumano methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:11'!miFicha	^ficha.! !!JugadorHumano methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:31'!ficha: aString 	ficha := Ficha de: aString! !!JugadorHumano methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:21'!dondeJuego	^miProximaCelda! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!JugadorHumano class	instanceVariableNames: ''!!JugadorHumano class methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:31'!ficha: unaFicha 	|result|	result := self new.		result ficha: unaFicha.		^result.! !Object subclass: #Linea3	instanceVariableNames: 'celdas'	classVariableNames: ''	package: 'TTT'!!Linea3 methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:01'!initialize 	celdas := OrderedCollection new.! !!Linea3 methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:10'!todasLasFichasSonDe: unaFicha  	^celdas allSatisfy: [ :c | c contiene: unaFicha  ] .! !!Linea3 methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:01'!con: aCelda y: aCelda2 y: aCelda3 	celdas addAll: { aCelda . aCelda2 . aCelda3 }.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Linea3 class	instanceVariableNames: ''!!Linea3 class methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:00'!con: aCelda y: aCelda2 y: aCelda3 	|result|	result := self new.		result con: aCelda y: aCelda2 y: aCelda3.		^result.! !Error subclass: #NoExisteCelda	instanceVariableNames: ''	classVariableNames: ''	package: 'TTT'!Object subclass: #Tablero	instanceVariableNames: 'celdas ficha celda lineas'	classVariableNames: ''	package: 'TTT'!!Tablero methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 18:44'!buscarCelda: aDir 	^celdas detect: [ :c | c es: aDir ] ifNone: [ nil ] .! !!Tablero methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:11'!filaUnoCompleta: aJugador	 	ficha := aJugador miFicha.		1 to: 3 do: [ :c |		celda := self buscarCelda: (DirCelda f: 1 c: c).				(celda contiene: ficha) ifFalse: [ 			^false.		].	].		^true.! !!Tablero methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:21'!pone: aJugador en: aDir 	| c donde |		"Estos pasos surgen a partir de generalizar la interraccion entre Tablero <-> Jugador"	aJugador proximaJugada: aDir.		donde := aJugador dondeJuego.		c :=	self buscarCelda: donde.			(c isNil) ifTrue: [		NoExisteCelda signal. 	].	c contenido: (aJugador miFicha).! !!Tablero methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:11'!haCompletadoLinea: aJugador 	^lineas anySatisfy: [ :l | l todasLasFichasSonDe: (aJugador miFicha) ].	! !!Tablero methodsFor: 'as yet unclassified' stamp: 'DS 6/24/2021 21:21'!pone: aJugador 	| c donde |		"Estos pasos surgen a partir de generalizar la interraccion entre Tablero <-> Jugador"	donde := aJugador dondeJuego.		c :=	self buscarCelda: donde.			(c isNil) ifTrue: [		NoExisteCelda signal. 	].	c contenido: (aJugador miFicha).! !!Tablero methodsFor: 'initialization' stamp: 'DS 6/24/2021 21:07'!initialize 	| c11 c12 c13 c21 c22 c23 c31 c32 c33 |	celdas := OrderedCollection new.	lineas := OrderedCollection new.				c11 := Celda dir: (DirCelda f:1 c: 1).	c12 := Celda dir: (DirCelda f:1 c: 2).	c13 := Celda dir: (DirCelda f:1 c: 3).	c21 := Celda dir: (DirCelda f:2 c: 1).	c22 := Celda dir: (DirCelda f:2 c: 2).	c23 := Celda dir: (DirCelda f:2 c: 3).		c31 := Celda dir: (DirCelda f:3 c: 1).	c32 := Celda dir: (DirCelda f:3 c: 2).	c33 := Celda dir: (DirCelda f:3 c: 3).		celdas add: c11.	celdas add: c12.	celdas add: c13.			celdas add: c21.	celdas add: c22.	celdas add: c23.			celdas add: c31.	celdas add: c32.	celdas add: c33.		"Filas"	lineas add: (Linea3 con: c11 y: c12 y: c13).	lineas add: (Linea3 con: c21 y: c22 y: c23).	lineas add: (Linea3 con: c31 y: c32 y: c33).		"Cols"	lineas add: (Linea3 con: c11 y: c21 y: c31).	lineas add: (Linea3 con: c12 y: c22 y: c32).	lineas add: (Linea3 con: c13 y: c23 y: c33).		"Diag"	lineas add: (Linea3 con: c11 y: c22 y: c33).	lineas add: (Linea3 con: c31 y: c22 y: c13).	! !